    "version": "0.2.0",
    "configurations": [
        {
            "name": "Load",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}",
            "args": [
                "-V",
                "--yes",
                "load",
                "launchpad.yaml"
            ]
        },
        {
            "name": "Save",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}",
            "args": [
                "-V",
                "save",
                "--config",
                "/tmp/lp.yml"
            ]
        },
        {
            "name": "Default",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}",
            "args": [
                "-V",
                "default"
            ]
        },
        {
            "name": "Decode",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}/test"
        },
        {
            "name": "Dock",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}/dock"
        },
        {
            "name": "Desktop",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": false,
            "program": "${workspaceRoot}/desktop"
        }
    ]
}